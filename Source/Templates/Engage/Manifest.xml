<?xml version="1.0" encoding="utf-8" ?>
<EngageManifest Module="Rotator" Type="List">
  <Title>Engage</Title>
  <Description>
    This displays the content images (each 566px wide) with the titles floating on the right with a background image.
    Each title is 41px high, so the content image should be 205 px tall if there are five items, and otherwise adjusted accordingly.
    Not intended to be used with the next, previous, or pause buttons, or the position thumbnail.
  </Description>
  <PreviewImage>Preview.png</PreviewImage>
  <Stylesheet>Style.css</Stylesheet>
  <Template>Template.htm</Template>
  <ResourceFile>App_LocalResources\Template.resx</ResourceFile>
  <Settings>
    <Setting>
      <Name>ShowContentHeader</Name>
      <Value>False</Value>
    </Setting>
    <Setting>
      <Name>ShowContentHeaderLink</Name>
      <Value>False</Value>
    </Setting>
    <Setting>
      <Name>ShowReadMoreLink</Name>
      <Value>False</Value>
    </Setting>
    <Setting>
      <Name>ControlsShowNextButton</Name>
      <Value>False</Value>
    </Setting>
    <Setting>
      <Name>ControlsShowPrevButton</Name>
      <Value>False</Value>
    </Setting>
    <Setting>
      <Name>ControlsShowPauseButton</Name>
      <Value>False</Value>
    </Setting>
    <Setting>
      <Name>UseAnimations</Name>
      <Value>True</Value>
    </Setting>
    <Setting>
      <Name>AnimationPauseOnMouseOver</Name>
      <Value>True</Value>
    </Setting>
    <Setting>
      <Name>RotatorWidth</Name>
      <Value/>
    </Setting>
    <Setting>
      <Name>RotatorHeight</Name>
      <Value/>
    </Setting>
    <Setting>
      <Name>ThumbnailWidth</Name>
      <Value/>
    </Setting>
    <Setting>
      <Name>ThumbnailHeight</Name>
      <Value/>
    </Setting>
    <Setting>
      <Name>ControlsTitleDisplayMode</Name>
      <Value>RotateContent</Value>
    </Setting>
    <Setting>
      <Name>ContentTitleDisplayMode</Name>
      <Value>None</Value>
    </Setting>
    <Setting>
      <Name>ContentDisplayMode</Name>
      <Value>None</Value>
    </Setting>
    <Setting>
      <Name>ThumbnailDisplayMode</Name>
      <Value>Content</Value>
    </Setting>
    <Setting>
      <Name>PositionThumbnailDisplayMode</Name>
      <Value>None</Value>
    </Setting>
  </Settings>
</EngageManifest>